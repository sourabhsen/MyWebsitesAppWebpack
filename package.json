{
  "name": "mywebsite",
  "version": "0.1.0",
  "description": "website of my portal",
  "main": "index.js",
  "engines": {
    "node": "10.14.2"
  },
  "scripts": {
    "start": "npm run serve",
    "build:dist": "npm run build:server & npm run build:dev",
    "build:server": "babel -d ./dist/server ./app -s",
    "serve": "webpack-dev-server --inline --progress",
    "build": "tsc --rootDir src --outDir dist",
    "build:dev": "webpack --progress",
    "build:prod": "webpack -p --progress",
    "postinstall": "typings install",
    "clean:client": "rimraf dist/client",
    "clean:server": "rimraf dist/server",
    "clean:dist": "rimraf dist",
    "pretest": "npm run build:dist",
    "test": "karma start karma.conf.js"
  },
